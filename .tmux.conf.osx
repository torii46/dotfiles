# This file is for Mac dependencies of tmux configure

# Enable clipboard share if running OS is Mac
set -g default-command "reattach-to-user-namespace -l zsh"

bind-key -t vi-copy Enter copy-pipe "reattach-tu-user-namespace pbcopy" 
bind-key -t vi-copy y     copy-pipe "reattach-tu-user-namespace pbcopy" 
